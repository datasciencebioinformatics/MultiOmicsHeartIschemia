variables<-c("C12_1","PC_ae_C34_2","SM_C24_0","SM_OH_C22_1","SM_OH_C24_1","lysoPC_a_C18_2","PC_ae_C34_3","SM_C26_1","hsa-miR-10a","hsa-miR-1274b","hsa-miR-146b-5p","hsa-miR-212","hsa-miR-335","hsa-miR-500b","hsTroponinI","Pro_BNP")
##########################################################################################################################################

